<!DOCTYPE validators PUBLIC "http://OpenSymphony Group//XWork Validator 1.0.2//EN"
       "http://struts.apache.org/dtds/xwork-validator-1.0.2.dtd">
<validators>

	<field name="titulo">
		<field-validator type="stringlength">
			<param name="minLength">1</param>
			<message key="requiredString" />
		</field-validator>
	</field>
	<field name="selectedTitle">
		<field-validator type="stringlength">
			<param name="minLength">1</param>
			<message key="requiredString" />
		</field-validator>
	</field>
	<field name="tipo_general_especifica">
		<field-validator type="stringlength">
			<param name="minLength">1</param>
			<message key="requiredString" />
		</field-validator>
	</field>
	<field name="objetivos">
		<field-validator type="stringlength">
			<param name="minLength">1</param>
			<message key="requiredString" />
		</field-validator>
	</field>
	<field name="descripcion">
		<field-validator type="stringlength">
			<param name="minLength">1</param>
			<param name="trim">true</param>
			<message key="requiredString" />
		</field-validator>
	</field>
	<field name="conocimientos_previos">
		<field-validator type="stringlength">
			<param name="minLength">1</param>
			<param name="trim">true</param>
			<message key="requiredString" />
		</field-validator>
	</field>

</validators>